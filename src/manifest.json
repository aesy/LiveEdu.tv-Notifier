{
  "name": "LiveCoding.tv Notifier",
  "description": "Browser extension for LiveCoding.tv which lets you know when your favourite streamers go live!",
  "version": "0.9.0",
  "homepage_url": "https://github.com/easyfuckingpeasy/LiveCoding.tv-Notifier",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "19": "img/19_2.png",
      "38": "img/38_2.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/32_1.png",
    "48": "img/48_1.png",
    "128": "img/128_1.png"
  },
  "permissions": [
    "storage",
    "background",
    "https://www.livecoding.tv/o/token/",
    "https://www.livecoding.tv/*",
    "notifications",
    "tabs"
  ],
  "web_accessible_resources": [
    "message.html"
  ],
  "manifest_version": 2,
  "update_url": "https://clients2.google.com/service/update2/crx"
}
